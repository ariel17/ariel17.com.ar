<div class="row col-md-offset-2 col-md-8">
  <% page.posts.each(function(post){ %>
    <h2>
      <a href="<%- url_for(post.path) %>"><%= post.title %></a>
      <small><%= date(post.date, config.date_format) %></small>
    </h2>
    <div class="col-md-offset-2 col-md-8 col-sm-12">
      <p><%- post.excerpt %></p>
    </div>
  <% }) %>
</div>
